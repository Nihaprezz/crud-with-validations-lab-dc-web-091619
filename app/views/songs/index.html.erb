<h1>All Songs</h1>

<tbody>
        <% @songs.each do |song| %>
          <tr>
            <td><%= link_to song.title, song_path(song) %></td>
            <td><%= song.released %></td>
            <td><%= song.release_year %></td>
            <td><%= song.genre %></td>
            <td><%= song.artist_name %></td>
          </tr>
        <% end %>
</tbody> %>

<button>
    <%= link_to "Add New Song", new_song_path%>
</button>